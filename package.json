{
  "name": "ad-hok",
  "version": "0.1.1",
  "description": "Recompose-style React hooks",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "generateFlowMaxType": "node scripts/generateFlowMaxType.js > src/flowMaxType.ts && prettier --write src/flowMaxType.ts"
  },
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "keywords": [
    "composition",
    "utilities",
    "toolkit",
    "react",
    "hooks",
    "react-hooks",
    "recompose"
  ],
  "author": "Julian Rosse <@helixbass> <julian@helixbass.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:helixbass/ad-hok.git"
  },
  "bugs": {
    "url": "https://github.com/helixbass/ad-hok/issues"
  },
  "homepage": "https://github.omc/helixbass/ad-hok",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.41",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.0.1",
    "jest-dom": "^2.1.0",
    "lodash": "^4.17.15",
    "prettier": "~2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^26.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
